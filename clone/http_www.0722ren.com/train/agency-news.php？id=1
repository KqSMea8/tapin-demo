$(document).ready(function(){
	var train_id="1";
	var tsTimeStamp= new Date().getTime();
	$.get("/plus/ajax_contact.php", { "id": train_id,"time":tsTimeStamp,"act":"train_contact"},
		function (data,textStatus)
		 {			
			$("#train_contact").html(data);
		 }
	);
	$(".newsmore").click(function(){
		$(".newsmore").show();
		$(".newsmore").val('正在加载，请稍后...');
		var offset=$(".listbox:last-child").attr('id');
		offset=parseInt(offset.substr(3));
		var tsTimeStamp= new Date().getTime();
		$.get("/plus/ajax_comajaxlist.php", { "trainid": train_id,"time":tsTimeStamp,"act":"show_news_more","offset":offset+5,"rows":"5"},function (data,textStatus)
			{
				if (data=='empty!')
				{
					$(".newsmore").val('已加载所有数据！');
					//$(".comment_more").unbind();
				}
				else
				{
					$(".newsmore").val('查看更多');
					$(".news ul").append(data);
				}
			}
		);
	});
});
</script>
